<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Sts\KafkaBundle\Configuration\Type\Topics" id="sts_kafka.configuration.topics">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\GroupId" id="sts_kafka.configuration.group.id">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\Brokers" id="sts_kafka.configuration.brokers">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\OffsetStoreMethod"
                 id="sts_kafka.configuration.offset.store.method">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\Partition" id="sts_kafka.configuration.partition">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\Offset" id="sts_kafka.configuration.offset">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\Timeout" id="sts_kafka.configuration.timeout">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\AutoOffsetReset"
                 id="sts_kafka.configuration.auto.offset.reset">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\AutoCommitIntervalMs"
                 id="sts_kafka.configuration.auto.commit.interval.ms">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\Decoder" id="sts_kafka.configuration.decoder">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\SchemaRegistry" id="sts_kafka.configuration.schema.registry">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\EnableAutoOffsetStore"
                 id="sts_kafka.configuration.enable.auto.offset.store">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\EnableAutoCommit"
                 id="sts_kafka.configuration.enable.auto.commit">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\LogLevel" id="sts_kafka.configuration.log.level">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\RegisterMissingSchemas"
                 id="sts_kafka.configuration.register.missing.schemas">
            <tag name="sts_kafka.configuration.type"/>
        </service>
        <service class="Sts\KafkaBundle\Configuration\Type\RegisterMissingSubjects"
                 id="sts_kafka.configuration.register.missing.subjects">
            <tag name="sts_kafka.configuration.type"/>
        </service>
    </services>
</container>
