<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Sts\KafkaBundle\Validator\Validator" id="sts_kafka.validator.validator">
            <argument type="tagged_iterator" tag="sts_kafka.validator"/>
        </service>
        <service class="Sts\KafkaBundle\Validator\Type\PlainValidator" id="sts_kafka.validator.type.plain_validator">
            <tag name="sts_kafka.validator"/>
        </service>
    </services>
</container>
