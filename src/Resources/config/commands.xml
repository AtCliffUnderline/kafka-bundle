<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Sts\KafkaBundle\Command\ConsumeCommand" id="sts_kafka.consume_command">
            <tag name="console.command"/>
            <argument type="service" id="sts_kafka.configuration.raw_configuration"/>
            <argument type="service" id="sts_kafka.client.consumer.consumer_provider"/>
            <argument type="service" id="sts_kafka.configuration.configuration_resolver"/>
            <argument type="service" id="sts_kafka.client.consumer.consumer_client"/>
        </service>
        <service class="Sts\KafkaBundle\Command\ProducersDescribeCommand"
                 id="sts_kafka.command.describe_producers_command">
            <tag name="console.command"/>
            <argument type="service" id="sts_kafka.client.producer.producer_provider"/>
            <argument type="service" id="sts_kafka.configuration.configuration_resolver"/>
        </service>
    </services>
</container>
