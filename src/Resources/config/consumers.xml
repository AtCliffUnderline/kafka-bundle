<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Sts\KafkaBundle\Client\Consumer\ConsumerProvider" id="sts_kafka.client.consumer.consumer_provider"/>
        <service id="Sts\KafkaBundle\Client\Consumer\ConsumerProvider" alias="sts_kafka.client.consumer.consumer_provider"/>
        <service class="Sts\KafkaBundle\Client\Consumer\ConsumerClient" id="sts_kafka.client.consumer.consumer_client" shared="false">
            <argument type="service" id="sts_kafka.rd_kafka.kafka_configuration_factory"/>
            <argument type="service" id="sts_kafka.factory.message_factory"/>
            <argument type="service" id="sts_kafka.configuration.configuration_resolver"/>
        </service>
        <service id="Sts\KafkaBundle\Client\Consumer\ConsumerClient" alias="sts_kafka.client.consumer.consumer_client"/>
    </services>
</container>
