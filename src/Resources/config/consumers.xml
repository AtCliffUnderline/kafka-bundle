<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Sts\KafkaBundle\Consumer\ConsumerProvider" id="sts_kafka.consumer.provider"/>
        <service id="Sts\KafkaBundle\Consumer\ConsumerProvider" alias="sts_kafka.consumer.provider"/>
        <service class="Sts\KafkaBundle\Consumer\Client\ConsumerClient" id="sts_kafka.consumer.client">
            <argument type="service" id="sts_kafka.rd.kafka.consumer.factory"/>
            <argument type="service" id="sts_kafka.rd.kafka.topic.configuration.factory"/>
            <argument type="service" id="sts_kafka.rd.kafka.context.factory"/>
            <argument type="service" id="sts_kafka.message.factory"/>
        </service>
        <service id="Sts\KafkaBundle\Consumer\Client\ConsumerClient" alias="sts_kafka.consumer.client"/>
    </services>
</container>
