<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Sts\KafkaBundle\Factory\MessageFactory" id="sts_kafka.factory.message_factory">
            <argument type="tagged_iterator" tag="sts_kafka.decoder"/>
            <argument type="tagged_iterator" tag="sts_kafka.denormalizer"/>
            <argument type="service" id="sts_kafka.validator.validator"/>
        </service>
        <service id="Sts\KafkaBundle\Factory\MessageFactory" alias="sts_kafka.factory.message_factory"/>
    </services>
</container>
