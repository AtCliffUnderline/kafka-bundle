{
  "name": "sts/kafka-bundle",
  "description": "Bundle to consume and produce messages from/to Apache Kafka.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Mariusz Jeruzal",
      "email": "mariusz.jeruzal@sts.pl"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-rdkafka": "*",
    "symfony/config": "^5.0",
    "symfony/dependency-injection": "^5.2",
    "symfony/http-kernel": "^5.2.1",
    "symfony/console": "5.2.*",
    "symfony/options-resolver": "^5.2",
    "flix-tech/avro-serde-php": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Sts\\KafkaBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sts\\KafkaBundle\\Tests\\": "Tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^5.2",
    "symfony/framework-bundle": "^5.2",
    "symfony/dotenv": "5.2.*",
    "phpstan/phpstan": "^0.12.75",
    "phpstan/phpstan-symfony": "^0.12.18",
    "phpstan/phpstan-phpunit": "^0.12.17",
    "squizlabs/php_codesniffer": "^3.5"
  }
}
